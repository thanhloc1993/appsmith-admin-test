{
    "name": "eibanam",
    "version": "0.0.1",
    "description": "Mother of E2E Tests",
    "main": "README.md",
    "repository": "git@github.com:manabie-com/eibanam.git",
    "author": "dev@manabie.com",
    "license": "Private",
    "private": true,
    "scripts": {
        "cucumber": "yarn screen-test && npx ts-node configurations/run-cucumber/index.ts run-e2e",
        "screen-test": "npx playwright test test-suites/squads/architecture/recorders/management-configuration.ts",
        "format": "eslint --fix . && prettier --write .",
        "unit-test": "jest",
        "bdd-lint": "gherkin-lint",
        "ts-lint": "tsc -p tsconfig.json",
        "postinstall": "husky install",
        "mana-cli:formatter": "yarn mana --config ./drivers/message-formatter/mana.config.ts",
        "metadata-export:formatter": "yarn mana-cli:formatter hasura metadata export",
        "metadata-export:local:formatter": "yarn mana-cli:formatter hasura metadata export --use-local true",
        "graphql-export:formatter": "yarn mana-cli:formatter graphql query export",
        "schema-export:formatter": "yarn mana-cli:formatter hasura schema export --use-local true",
        "typescript-export:formatter": "yarn schema-export:formatter && yarn mana-cli:formatter graphql ts export",
        "allow-list:formatter": "yarn metadata-export:formatter && yarn mana-cli:formatter hasura allow-list update",
        "metadata:export": "yarn mana hasura metadata export",
        "metadata:export:local": "yarn mana hasura metadata export --use-local true",
        "metadata:import:local": "yarn mana hasura metadata import",
        "graphql:export": "yarn mana graphql query export",
        "schema:export": "yarn mana hasura schema export",
        "schema:export:local": "yarn mana hasura schema export --use-local true",
        "typescript:export": "yarn schema:export && yarn mana graphql ts export",
        "allow-list:update": "yarn mana hasura allow-list update",
        "allow-list": "yarn metadata:export:local && yarn allow-list:update"
    },
    "lint-staged": {
        "*.{js,ts}": [
            "yarn format",
            "git add ."
        ],
        "*.feature": [
            "yarn bdd-lint",
            "git add ."
        ]
    },
    "jira-prepare-commit-msg": {
        "messagePattern": "$J $M",
        "jiraTicketPattern": "(LT-[0-9]+)",
        "commentChar": "#",
        "isConventionalCommit": false,
        "allowEmptyCommitMessage": false
    },
    "dependencies": {
        "@apollo/client": "^3.4.16",
        "@cucumber/cucumber": "8.5.1",
        "@google-cloud/storage": "5.16.1",
        "@improbable-eng/grpc-web": "^0.14.0",
        "@improbable-eng/grpc-web-node-http-transport": "^0.14.0",
        "@playwright/test": "1.27.1",
        "@types/draft-js": "^0.11.3",
        "axios": "^0.21.2",
        "cross-fetch": "^3.1.5",
        "crypto-js": "4.1.1",
        "draft-js": "^0.11.7",
        "flutter-driver-x": "file:./libraries/flutter-driver-x-js",
        "gherkin-lint": "4.2.2",
        "google-libphonenumber": "3.2.27",
        "google-protobuf": "^3.14.0",
        "graphql": "^15.6.1",
        "graphql-tag": "^2.12.5",
        "https": "1.0.0",
        "moment-timezone": "^0.5.35",
        "nock": "13.2.2",
        "papaparse": "5.3.2",
        "playwright": "1.27.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "ts-enum-util": "^4.0.2",
        "ts-retry-promise": "^0.6.0",
        "ulid": "^2.3.0",
        "unleash-client": "3.10.1"
    },
    "devDependencies": {
        "@trivago/prettier-plugin-sort-imports": "^2.0.2",
        "@types/crypto-js": "4.0.2",
        "@types/google-libphonenumber": "7.4.23",
        "@types/jest": "^26.0.23",
        "@types/node": "^15.12.2",
        "@types/papaparse": "5.3.3",
        "@typescript-eslint/eslint-plugin": "5.17.0",
        "@typescript-eslint/parser": "5.17.0",
        "eslint": "^7.30.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-filename-rules": "^1.2.0",
        "eslint-plugin-import": "^2.23.4",
        "eslint-plugin-prettier": "^3.4.0",
        "gherkin-parse": "1.0.6",
        "husky": "^6.0.0",
        "jest": "^27.0.6",
        "lint-staged": "^11.0.0",
        "prettier": "^2.3.2",
        "ts-jest": "^27.0.3",
        "ts-node": "^10.0.0",
        "tsconfig-paths": "^3.9.0",
        "tslint": "^5.12.1",
        "typedoc": "^0.21.4",
        "typedoc-github-wiki-theme": "^0.5.0",
        "typedoc-plugin-markdown": "^3.10.3",
        "typescript": "4.6.2"
    }
}
